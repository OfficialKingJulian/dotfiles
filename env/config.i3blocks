# i3blocks config file

separator_block_width=20
markup=none

# Generic media player support
[mediaplayer]
command=~/.config/i3blocks/inc/mediaplayer/mediaplayer
instance=spotify
interval=2
signal=10

# Volume indicator
[volume]
command=~/.config/i3blocks/inc/volume/volume
label=♪  
interval=1
signal=10

# Battery indicator
[battery]
command=~/.config/i3blocks/inc/battery/battery
label=  
interval=30

# Date Time
[time]
command=date '+%H:%M:%S %d-%m-%Y'
interval=1

